te from \"@material-ui/lab/Autocomplete\";\n\nexport default function GoogleMap() {\n  const [address, setAddress] = React.useState(\"\");\n  const [coordinates, setCoordinates] = React.useState({\n    lat: null,\n    lng: null,\n  });\n\n  const handleSelect = async (value) => {\n    const results = await geocodeByAddress(value);\n    const latLng = await getLatLng(results[0]);\n    setAddress(value);\n    setCoordinates(latLng);\n  };\n\n  return (\n    <div>\n      <PlacesAutocomplete\n        value={address}\n        onChange={setAddress}\n        onSelect={handleSelect}\n      >\n        {({ getInputProps, suggestions, getSuggestionItemProps, loading }) => (\n          <div>\n            <input {...getInputProps({ placeholder: \"City\" })} />\n\n            <div className=\"autocomplete-dropdown-container\">\n              {loading ? <div>...loading</div> : null}\n\n              {suggestions.map((suggestion) => {\n                const style = {\n                  backgroundColor: suggestion.active ? \"##d8f0f0\" : \"#fff\",\n                  width: \n                  suggestion.active ?\n                  \"300px\" : \"300px\",\n                  padding: \"10px\",\n                  color: \"#b4aeae\"\n                };\n\n                return (\n                  <div {...getSuggestionItemProps(suggestion, { style })}>\n                    {suggestion.description}\n                  </div>\n                );\n              })}\n            </div>\n          </div>\n        )}\n      </PlacesAutocomplete>\n    </div>\n  );\n}\n","/Users/oxanachernikova/Documents/REACT/SportsHood/SportsHood/Client/src/components/Navbar/NavbarStyled.js",[],"/Users/oxanachernikova/Documents/REACT/SportsHood/SportsHood/Client/src/components/Modal/Modal.js",["97","98","99"],"import React, { useRef, useEffect, useCallback } from \"react\";\nimport { useSpring, animated } from \"react-spring\";\nimport styled from \"styled-components\";\nimport { MdClose } from \"react-icons/